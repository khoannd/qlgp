<script>
        function viewReadonlyParishioner(id) {
            $("#viewParishionerId").val(id);
            var parishionerDomicile = $("#hiddenParishionerDomicile").val();
            startLoading();
            $.ajax({
                url: "/Parishioner/LoadFullParishioner",
                type: "POST",
                data: { id: id },
                success: function (data) {

                    var parishioner = data.parishioner;
                    var baptism = data.baptism;
                    var holyCommunion = data.holyCommunion;
                    var confirmation = data.confirmation;
                    var vocation = data.vocation;


                    $("#viewReadonlyTxtParishionerCodeModal").val(parishioner.Code);

                    $("#viewReadonlyTxtParishionerCodeModal").attr('readonly', true);
                    $("#viewReadonlyTxtParishionerCodeModal").show();

                    $("#viewReadonlyTxtChristianNameModal").val(parishioner.ChristianName);
                    $("#viewReadonlyTxtParishionerNameModal").val(parishioner.Name);
                    $("#viewReadonlyCbGenderModal").val(parishioner.Gender);

                    $("#viewReadonlyTxtBirthDateModal").val(parishioner.BirthDate);
                    $("#viewReadonlyTxtBirthPlaceModal").val(parishioner.BirthPlace);

                    $("#viewReadonlyTxtCommunityNameModal").val(data.communityName);
                    $("#viewReadonlyTxtCommunityNameModal").change();
                    $("#viewReadonlyCbIsCountedModal").prop("checked", !parishioner.IsCounted);

                    $("#viewReadonlyTxtFatherNameModal").val(parishioner.FatherName);
                    $("#viewReadonlyTxtMotherNameModal").val(parishioner.MotherName);

                    if (parishionerDomicile == 1) {
                        $("#viewReadonlyCbbDomicileStatusModal").val(1);
                        $("#viewReadonlyCbbDomicileStatusModal").change();
                        $("#viewReadonlyTxtDomicilePlaceModal").val(data.parishName);
                    } else {
                        $("#viewReadonlyCbbDomicileStatusModal").val(parishioner.DomicileStatus);
                        $("#viewReadonlyCbbDomicileStatusModal").change();
                        $("#viewReadonlyTxtDomicilePlaceModal").val(parishioner.DomicilePlace);
                    }                    

                    $("#viewReadonlyTxtEducationModal").val(parishioner.Education);
                    $("#viewReadonlyTxtCareerModal").val(parishioner.Career);
                    $("#viewReadonlyTxtAddressModal").val(parishioner.Address);
                    $("#viewReadonlyTxtPhoneModal").val(parishioner.Phone);
                    $("#viewReadonlyTxtMobilePhoneModal").val(parishioner.MobilePhone);
                    $("#viewReadonlyTxtEmailModal").val(parishioner.Email);
                    $("#viewReadonlyTxtNoteModal").val(parishioner.Note);

                    $("#viewReadonlyCbIsStudyingModal").prop("checked", parishioner.IsStudying);
                    $("#viewReadonlyCbIsCatechumenModal").prop("checked", parishioner.IsCatechumen);
                    $("#viewReadonlyCbIsMarriedModal").prop("checked", parishioner.IsMarried);
                    $("#viewReadonlyCbIsMarriedModal").change();
                    $("#viewReadonlyCbIsSingleModal").prop("checked", parishioner.IsSingle);
                    $("#viewReadonlyCbIsDeadModal").prop("checked", parishioner.IsDead);
                    $("#viewReadonlyCbIsDeadModal").change();
                    $("#viewReadonlyTxtDeadDateModal").val(parishioner.DeadDate);

                    if (baptism) {
                        $("#txtHiddenBaptismId").val(baptism.Id);
                        $("#viewReadonlyTxtBaptismDateModal").val(baptism.Date);
                        $("#viewReadonlyTxtBaptismNumberModal").val(baptism.Number);
                        $("#viewReadonlyTxtBaptismPlaceModal").val(baptism.ReceivedPlace);
                        $("#viewReadonlyTxtBaptismPatronModal").val(baptism.Patron);
                        $("#viewReadonlyTxtBaptismGiverModal").val(baptism.Giver);
                    }

                    if (holyCommunion) {
                        $("#txtHiddenHolyCommunionId").val(holyCommunion.Id);
                        $("#viewReadonlyTxtHolyCommunionDateModal").val(holyCommunion.Date);
                        $("#viewReadonlyTxtHolyCommunionNumberModal").val(holyCommunion.Number);
                        $("#viewReadonlyTxtHolyCommunionPlaceModal").val(holyCommunion.ReceivedPlace);
                        $("#viewReadonlyTxtHolyCommunionGiverModal").val(holyCommunion.Giver);
                    }

                    if (confirmation) {
                        $("#txtHiddenConfirmationId").val(confirmation.Id);
                        $("#viewReadonlyTxtConfirmationDateModal").val(confirmation.Date);
                        $("#viewReadonlyTxtConfirmationNumberModal").val(confirmation.Number);
                        $("#viewReadonlyTxtConfirmationPlaceModal").val(confirmation.ReceivedPlace);
                        $("#viewReadonlyTxtConfirmationPatronModal").val(confirmation.Patron);
                        $("#viewReadonlyTxtConfirmationGiverModal").val(confirmation.Giver);
                    }



                    if (vocation) {
                        $("#txtHiddenVocationId").val(data.vocationId);
                        $("#viewReadonlyDate1").val(vocation.Date1);
                        $("#viewReadonlyDate2").val(vocation.Date2);
                        $("#viewReadonlyDate3").val(vocation.Date3);
                        $("#viewReadonlyDate4").val(vocation.Date4);
                        $("#viewReadonlyDate5").val(vocation.Date5);
                        $("#viewReadonlyDate6").val(vocation.Date6);
                        $("#viewReadonlyDate7").val(vocation.Date7);
                        $("#viewReadonlyDate8").val(vocation.Date8);
                        $("#viewReadonlyDate9").val(vocation.Date9);

                        $("#viewReadonlyTxtSeminary").val(vocation.Seminary);
                        $("#viewReadonlyCbbPosition").val(vocation.Position);
                        $("#viewReadonlytxtSeminaryAddress").val(vocation.SeminaryAddress);
                        $("#viewReadonlyTxtServedPlace").val(vocation.ServedPlace);
                        $("#viewReadonlyTxtServedPhone").val(vocation.Phone);
                        $("#viewReadonlyTxtServedAddress").val(vocation.ServedAddress);
                        $("#viewReadonlyTxtServedEmail").val(vocation.Email);
                        $("#viewReadonlyTxtVocationNote").val(vocation.Note);
                        $("#viewReadonlyCbIsLeft").prop("checked", vocation.IsLeft);
                    }
                    ViewReadonlyMatrimonyTable();
                    $("#viewReadonlyParishionerModal").modal('show');

                    loadParishionerClassesSearch(id);
                    loadParishionerSocietiesSearch(id);
                },
            });

