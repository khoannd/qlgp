                        var quantity = $("#viewReadonlyMatrimonyQuantity").val();

                        if (quantity > 0) {
                            $("#cbGender").attr("disabled", true); //
                            $("#cbIsMarried").prop("checked", true); //
                            $("#cbIsMarried").attr("disabled", true); //
                        }

                    }

                }
            });

