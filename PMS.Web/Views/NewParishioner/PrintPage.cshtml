<body>
    <div id="txtPrintArea" style="width: 100%;">
    </div>
    <script>
        var ids = sessionStorage.getItem("ids");

        getPrint();
        function getPrint() {
            if (ids == null || ids.length == 0) {
                return;
            }
            printSacrament();

        }

        function printSacrament() {
            ids = JSON.parse(ids);
            $.ajax({
                url: "/Priest/PrintPriest",
                type: "POST",
                traditional: true,
                data: {
                    'ids': ids
                },
                success: function (data) {
                    var template = data.template;
                    var result = data.result;
                    for (i in result) {
                        var item = result[i];
                        var temp = template;
                        if (item.ImageURL && item.ImageURL.indexOf(".") > 0) {
                            temp = temp.replace(/\[ImageURL]/g, item.ImageURL);
                        }
                        else {
                            temp = temp.replace(/\[ImageURL]/g, "/Images/no-img-profile-male.jpg");
                        }

                        
                        temp = temp.replace(/\[ImageName]/g, "Avatar " + item.Name);
                        temp = temp.replace(/\[PriestName]/g, item.ChristianName + " " + item.Name);
                        temp = temp.replace(/\[Code]/g, item.Code);
                        if (item.Address == null) {
                            temp = temp.replace(/\[Address]/g, "");
                        } else {
                            temp = temp.replace(/\[Address]/g, item.Address);
                        }

                        if (item.Phone == null) {
                            temp = temp.replace(/\[Phone]/g, "");
                        } else {
                            temp = temp.replace(/\[Phone]/g, item.Phone);
                        }

                        if (item.IDNo == null) {
                            temp = temp.replace(/\[IDNo]/g, "&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;");
                        } else {
                            temp = temp.replace(/\[IDNo]/g, item.IDNo);
                        }
                        if (item.IDDate == null || item.IDDate == "") {
                            temp = temp.replace(/\[IDDate]/g, "&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;");
                        } else {
                            temp = temp.replace(/\[IDDate]/g, item.IDDate);
                        }

                        if (item.IDPlace == null) {
                            temp = temp.replace(/\[IDPlace]/g, "");
                        } else {
                            temp = temp.replace(/\[IDPlace]/g, item.IDPlace);
                        }

                        if (item.OrdinationDate == null) {
                            temp = temp.replace(/\[OrdinationDate]/g, "");
                        } else {
                            temp = temp.replace(/\[OrdinationDate]/g, item.OrdinationDate);
                        }

                        if (item.BirthPlace == null) {
                            temp = temp.replace(/\[Birth]/g, item.BirthDate);
                        } else {
                            temp = temp.replace(/\[Birth]/g, item.BirthDate + " - " + item.BirthPlace);
                        }

                        if (item.Bishop == null) {
                            temp = temp.replace(/\[Bishop]/g, "");
                        } else {
                            temp = temp.replace(/\[Bishop]/g, item.Bishop);
                        }

                        temp = temp.replace(/\[DioceseName]/g, item.DioceseName);
                        temp = temp.replace(/\[CurrentDate]/g, data.now);
                        temp = temp.replace(/\[ThreeYearDate]/g, data.threeYear);
                        var s = "";
                        s = '<div style="width: auto; height: auto; margin-left: 50px;">' + temp + '</div>';
                        $("#txtPrintArea").append(temp);
                    }
                    window.print();
                },
            });
        }
    </script>
    <div class="print-icon-bottom"><a style="cursor: pointer" onclick="window.print()" title="Print"><img style="border: none" src="data:image/jpg;base64," alt="Print" width="24" /></a></div>
